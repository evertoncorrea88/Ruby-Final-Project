<h1>Log in</h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for  :admin,  url: {action: :verify} do |f| %>
            <%= f.label :email %>
            <%= f.text_field :email, class: 'form-control' %><br>

            <%= f.label :password %>
            <%= f.password_field :password, class: 'form-control' %><br>

            <%= f.submit "Log in", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>
    

