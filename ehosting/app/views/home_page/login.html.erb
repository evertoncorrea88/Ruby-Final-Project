

<h2>Login</h2>
<h3>
	<%= form_for  :admin,  url: {action: :verify} do |form| %>
		<p><label_for="email">Email:</label>
		<%= form.text_field  :email,  size: 20 %></p>
		<p><label_for = "password">Password:</label>
		<%= form.password_field  :password,  size: 20 %></p
		<p><%= submit_tag "Login" %></p>
	<% end %>
</h3>

